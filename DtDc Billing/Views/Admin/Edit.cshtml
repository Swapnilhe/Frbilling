@model DtDc_Billing.Models.FranchiseeModel
@using (Ajax.BeginForm("Edit", "Admin", new AjaxOptions
{ HttpMethod = "Post", UpdateTargetId = "frmEmp", InsertionMode = InsertionMode.Replace }, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="row">
        <div class="col-md-12">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h4>Edit Franchisee</h4>
                </div>
                <!-- /.box-header -->


                <div class="form-horizontal">
                    <div class="box-body">

                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                        @Html.HiddenFor(model => model.F_Id)


                        @Html.HiddenFor(model => model.PF_Code)
                        @Html.HiddenFor(model => model.password)
                        @Html.HiddenFor(model => model.Datetime_Fr)



                        <div class="form-group">
                            <label class="col-md-2">PF Code</label>
                            <div class="col-md-2">
                                @Html.TextBoxFor(model => model.PF_Code, new { @class = "form-control input-sm", @readonly = "readonly" })
                                @Html.ValidationMessageFor(model => model.PF_Code, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2"> Franchisee Name </label>
                            <div class="col-md-5">
                                @Html.EditorFor(model => model.Franchisee_Name, new { htmlAttributes = new { @class = "form-control input-sm" } })
                                @Html.ValidationMessageFor(model => model.Franchisee_Name, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2">Email ID</label>
                            <div class="col-md-5">
                                @Html.TextBoxFor(model => model.Sendermail, new { @class = "form-control input-sm", @readonly = "readonly" })
                                @Html.ValidationMessageFor(model => model.Sendermail, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2"> Franchisee Address</label>
                            <div class="col-md-5">
                                @Html.TextAreaFor(model => model.F_Address, new { @class = "form-control input-sm", @rows = 3 })
                                @Html.ValidationMessageFor(model => model.F_Address, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2"> Owner Name </label>
