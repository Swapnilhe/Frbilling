
@{

    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="content-wrapper">
    <section class="content">
        <div class="row">
            <div class="col-sm-12">
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h4>Import From Excel</h4>
                    </div>

                    <div class="form-horizontal">
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @using (Html.BeginForm("importFromExcel", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                        {
                                            <div class="col-md-3">
                                                <label for="file"><strong id="up-txt"> Upload for Document </strong></label>
                                            </div>
                                            <div class="col-md-6">
                                                <input type="file" name="httpPostedFileBase" id="file" class="form-control" />
                                            </div>
                                            <div class="col-md-1">
                                                <input type="submit" value="Upload" class="btn btn-primary" />
                                            </div>

                                            <div class="col-md-1">
                                                <a href="http://codetentacles-006-site44.htempurl.com/excelformat/UploadforDocument.xlsx" class="btn btn-success"> Export Excel</a>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                            <hr />

                        </div>
                    </div>


                    <div class="form-horizontal">
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @using (Html.BeginForm("importFromExcelWhole", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                        {
                                            <div class="col-md-3">
                                                <label for="file"><strong id="up-txt"> Upload for Non-Document </strong></label>
                                            </div>
                                            <div class="col-md-6">
                                                <input type="file" name="httpPostedFileBase" id="file" class="form-control" />
                                            </div>
                                            <div class="col-md-1">
                                                <input type="submit" value="Upload" class="btn btn-primary" />
                                            </div>

                                            <div class="col-md-1">
                                                <a href="http://codetentacles-006-site44.htempurl.com/excelformat/UploadforNonDocument.xlsx" class="btn btn-success"> Export Excel</a>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                            <hr />

                        </div>
                    </div>


                    <div class="form-horizontal">
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @using (Html.BeginForm("AddNewimporFromExcel", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                        {
                                            <div class="col-md-3">
                                                <label for="file"><strong id="up-txt"> Upload New </strong></label>
                                            </div>
                                            <div class="col-md-6">
                                                <input type="file" name="httpPostedFileBase" id="file" class="form-control" />
                                            </div>
                                            <div class="col-md-1">
                                                <input type="submit" value="Upload" class="btn btn-primary" />
                                            </div>

                                            <div class="col-md-1">
                                                <a href="http://codetentacles-006-site44.htempurl.com/excelformat/UploadNew.xlsx" class="btn btn-success"> Export Excel</a>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                            <hr />

                        </div>
                    </div>



                </div>
            </div>
        </div>
    </section>
    </div>

        @if (ViewBag.errormsg != null)
        {
            <script>
    alert('@ViewBag.errormsg');
            </script>
        }


        @if (ViewBag.Success != null)
        {
            <script>
    alert('@ViewBag.Success');
            </script>
        }

        @if (TempData["Success"] != null)
        {
            <script>
        alert('@TempData["Success"]');
            </script>
        }

        @if (TempData["Successnew"] != null)
        {
            <script>
        alert('@TempData["Successnew"]');
            </script>
        }

<script>
    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
        $('body').addClass("sidebar-mini sidebar-collapse");
    });
</script>